local nameBox = nil
local nbSelection = nil
addcmd('hovername',{},function(args, speaker)
	execCmd('unhovername')
	wait()
	CMDBAR = Instance.new("TextLabel")
	CMDBAR.Name = randomString()
	CMDBAR.Parent = PARENT
	CMDBAR.BackgroundTransparency = 1
	CMDBAR.Size = UDim2.new(0,200,0,30)
	CMDBAR.Font = Enum.Font.Code
	CMDBAR.TextSize = 16
	CMDBAR.Text = ""
	CMDBAR.TextColor3 = Color3.new(1, 1, 1)
	CMDBAR.TextStrokeTransparency = 0
	CMDBAR.TextXAlignment = Enum.TextXAlignment.Left
	CMDBAR.ZIndex = 10
	local function updateNameBox()
		local t
		local target = IYMouse.Target

		if target then
			local humanoid = target.Parent:FindFirstChild('Humanoid') or target.Parent.Parent:FindFirstChild('Humanoid')
			if humanoid then
				t = humanoid.Parent
			end
		end

		if t ~= nil then
			local x = IYMouse.X
			local y = IYMouse.Y
			local xP
			local yP
			if IYMouse.X > 200 then
				xP = x - 205
				nameBox.TextXAlignment = Enum.TextXAlignment.Right
			else
				xP = x + 25
				nameBox.TextXAlignment = Enum.TextXAlignment.Left
			end
			CMDBAR.Position = UDim2.new(0, xP, 0, y)
			CMDBAR.Text = t.Name
			nameBox.Visible = true
		else
			CMDBAR.Visible = false
		end
	end
	nbUpdateFunc = IYMouse.Move:Connect(updateNameBox)
end)
